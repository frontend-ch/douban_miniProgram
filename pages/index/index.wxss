/**index.wxss**/
@import "../../template/templates/tem.wxss";
.content{
  height: 100%;
  width: 100%
}
.search-input{
  display: flex;
  justify-content: center;
  width: 100%;
  height: 80rpx;
  background-color: #3AB245;
  align-items: center
}
.search-input input{
  width: 95%;
  height: 50rpx;
  background-color: white;
  border-radius: 8rpx;
  font-size: 28rpx
}
.titleclass{
  width: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis
}
.titleclass text{
  display: inline-block
}